		}

		onTime 300 {
			transition "black::backcolor" "1,1,1,0" "1,1,1,.2" "0" ;
		}

		onTime 400 {
			transition "black::backcolor" "1,1,1,.2" "1,1,1,0" "0" ;
		}

		onTime 500 {
			transition "black::backcolor" "1,1,1,0" "1,1,1,.1" "0" ;
		}

		onTime 600 {
			transition "black::backcolor" "1,1,1,.1" "1,1,1,0" "0" ;
		}

	}
}
