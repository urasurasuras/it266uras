	windowDef anim_empty
	{
		visible	1
		rect	0,0,1,1
		notime	1

		onTime 0 {
			set "t_valveopen::visible" "0"	;
			set "iris_arc1::visible" "0"	;
			set "iris_arc2::visible" "0"	;
			
			set "acid2::visible" "0"	;
			
			transition "iris::rotate" "0" "90" "7200" "500" "2000" ;
			transition "iris_circle::rect" "$iris_circle::rect" "$d_iris_circle::rect" "7200" "500" "1500" ;
			
			transition "acid::rect" "$acid::rect" "$d_acid::rect" "7200" "500" "1500" ;
			transition "bubbles::rect" "$bubbles::rect" "$d_bubbles::rect" "7200" "500" "1500" ;
			
			transition "arrow1::matcolor" "$desktop::white_34" "$desktop::white_1" "1200" ;
		}

		onTime 1200 {
			transition "arrow2::matcolor" "$desktop::white_34" "$desktop::white_1" "1200" ;
			
			transition "arrow1::matcolor" "$desktop::white_1" "$desktop::white_34" "1200" ;
		}

		onTime 2400 {
			transition "arrow3::matcolor" "$desktop::white_34" "$desktop::white_1" "1200" ;
			
			transition "arrow2::matcolor" "$desktop::white_1" "$desktop::white_34" "1200" ;
		}

		onTime 3600 {
			transition "arrow4::matcolor" "$desktop::white_34" "$desktop::white_1" "1200" ;
			
			transition "arrow3::matcolor" "$desktop::white_1" "$desktop::white_34" "1200" ;
		}

		onTime 4800 {
			transition "arrow5::matcolor" "$desktop::white_34" "$desktop::white_1" "1200" ;
			
			transition "arrow4::matcolor" "$desktop::white_1" "$desktop::white_34" "1200" ;
		}

		onTime 6000 {
			transition "arrow5::matcolor" "$desktop::white_1" "$desktop::white_34" "1200" ;
		}

		onTime 7200 {
			//set "iris_circle::visible" "0" ;
			set "acidwarn::visible" "1" ;
		}

	}
